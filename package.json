{
  "name": "@hexeo/vue-typescript-jsx",
  "version": "2.0.2",
  "description": "Translate React JSX props into Vue VDOM data format at runtime",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "env TS_NODE_FILES=1 TS_NODE_PRETTY=1 mocha --require ts-node/register test/**/*.tsx",
    "build": "tsc  --outDir dist",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heXeo/vue-typescript-jsx.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "jsx",
    "tsx"
  ],
  "author": "Leandre Gohy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heXeo/vue-typescript-jsx/issues"
  },
  "homepage": "https://github.com/heXeo/vue-typescript-jsx#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.0",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "mocha": "^6.0.2",
    "ts-node": "^8.0.3",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000",
    "vue": "^2.6.10"
  }
}
